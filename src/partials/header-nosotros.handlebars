<div class="oc-img-header">
    {{#if video}}
    <video class="oc-img-header__video d-none d-lg-block" src="{{video}}" autoplay muted loop></video>
    {{/if}}
    <img src="{{img}}" alt="" class="oc-img-header__img d-lg-none">
    <div class="oc-img-header__caption">
        <h1 class="oc-img-header__title-p">Nosotros</h1>
        <h2 class="oc-img-header__title-s">{{title}}</h2>
    </div>
</div>

<div class="oc-nosotros-submenu">
    {{>submenu itemsSubmenu="{{itemsSumenu}}"}}
</div>
            